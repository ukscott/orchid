{
  "name": "chiuk/php-test",
  "type": "project",
  "description": "A short coding test for potential developers.",
  "license": "MIT",
  "require": {
    "php": ">=7.3",
    "corneltek/getoptionkit": "^2.7"
  },
  "require-dev": {
    "phpunit/phpunit": "^9"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "./vendor/bin/phpunit tests",
    "app": "php ./run.php"
  }
}
